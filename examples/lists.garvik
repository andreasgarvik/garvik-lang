l = [1,2,3]
l[1]
// 2

m = [[1,2,3],[4,5,6]]
m[1][1]
// 5

x = [1,2,3]
f = l -> l[0] + l[1] + l[2]
f(x)
// 6

l = [4, x -> x + 2,[1,2,3]]
times = x ->
  let c = x[0] in
  let f = x[1] in
  let l = x[2] in
  if c == 0
    then l
  else
    times([c - 1, f, [f(l[0]),f(l[1]),f(l[2])]])
times(l)
// [9,10,11]

l = [0, x -> x * 2,[1,2,3]]
map = x ->
  let c = x[0] in
  let f = x[1] in
  let l = x[2] in
  if c == len l
    then l
  else
    map([c + 1, f, l[c] = (f(l[c]))])
map(l)
// [2,4,6]
